<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>

<Scene xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.control.UserMenuControl" width="200" height="200" >
	<GridPane >
		<Button text="Reset Password" onAction="#setPassword" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
		<Button text="View Bookings" onAction="#viewBookings" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
		<Label fx:id="lblUsername" GridPane.columnIndex="1" GridPane.rowIndex="0" />
		<Label fx:id="lblActive" GridPane.columnIndex="1" GridPane.rowIndex="1" />
	</GridPane>
</Scene>

